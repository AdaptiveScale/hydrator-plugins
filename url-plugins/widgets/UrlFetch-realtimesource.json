{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "Url Fetch Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Url",
          "name": "url"
        },
        {
          "widget-type": "number",
          "label": "Delay",
          "name": "delay",
          "widget-attributes" : {
            "default": "3600"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "ts": "long",
        "url": "string",
        "headers": "map<string, string>",
        "body": "string"
      }
    }
  ]
}
