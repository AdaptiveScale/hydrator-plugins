{
  "id": "CSVParser",
  "groups": {
    "position": [ "group1" ],
    "group1": {
      "display": "CSV Parser",
      "position": [ "field", "format" ],
      "fields": {
        "field": {
          "widget": "textbox",
          "label": "Input field to Parse",
          "properties": {
            "width": "large"
          }
        },
        "format": {
          "widget": "select",
          "label": "Formats",
          "properties": {
            "values" : [ "DEFAULT", "EXCEL", "MYSQL", "RFC4180", "TDF" ],
            "default": "DEFAULT"
          }
        }
      }
    }
  },
  "outputschema": {
    "schema" : {
      "widget": "schema",
      "schema-types" : [ "boolean", "int", "long", "float", "double", "bytes", "string" ],
      "schema-default-type" : "string",
      "property-watch": "format"
    }
  }
}
