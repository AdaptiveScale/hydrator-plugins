{
  "id": "Cassandra",
  "groups": {
    "position": [ "group1", "group2" ],
    "group1": {
      "display": "Cassandra Configuration",
      "position": [ "initialAddress", "port", "username", "password", "properties" ],
      "fields": {
        "initialAddress": {
          "widget": "textbox",
          "label": "Initial Address",
          "properties": {
            "width": "large"
          }
        },
        "port": {
          "widget": "textbox",
          "label": "Port",
          "properties": {
            "width": "medium"
          }
        },
        "username": {
          "widget": "textbox",
          "label": "Username",
          "properties": {
            "width": "medium"
          }
        },
        "password": {
          "widget": "password",
          "label": "Password"
        },
        "properties": {
          "widget": "keyvalue",
          "label": "Other Properties",
          "properties": {
            "showDelimiter": "false"
          }
        }
      }
    },
    "group2": {
      "display": "Keyspace and Column Family",
      "position": [ "keyspace", "partitioner", "columnFamily", "primaryKey", "columns", "query" ],
      "fields": {
        "keyspace": {
          "widget": "textbox",
          "label": "Keyspace",
          "properties": {
            "width": "medium"
          }
        },
        "partitioner": {
          "widget": "textbox",
          "label": "Partitioner",
          "properties": {
            "width": "large"
          }
        },
        "columnFamily": {
          "widget": "textbox",
          "label": "Column Family",
          "properties": {
            "width": "medium"
          }
        },
        "primaryKey": {
          "widget": "csv",
          "label": "Primary Key",
          "properties": {
            "delimiter": ","
          }
        },
        "columns": {
          "widget": "csv",
          "label": "Columns",
          "properties": {
            "delimiter": ","
          }
        },
        "query": {
          "widget": "textbox",
          "label": "CQL Query"
        }
      }
    }
  }
}
